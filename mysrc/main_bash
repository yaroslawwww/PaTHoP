#!/bin/bash

lower_bound=0.05
upper_bound=0.95
num_iterations=20
step=$(echo "scale=10; ($upper_bound - $lower_bound) / $num_iterations" | bc)
epsilon=$lower_bound

for i in $(seq 1 $num_iterations); do
  sbatch ./bash $epsilon
  epsilon=$(echo "scale=10; $epsilon + $step" | bc)
done

epsilon=$lower_bound

for i in $(seq 1 $num_iterations); do
  sbatch ./zero_case_bash $epsilon
  epsilon=$(echo "scale=10; $epsilon + $step" | bc)
done